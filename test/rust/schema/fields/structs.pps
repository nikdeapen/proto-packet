// A struct with unsigned integers.
struct UnsignedInts {

    // A `u8` field.
    one: u8;

    // A `u16` field.
    two: u16;

    // A `u32` field.
    three: u32;

    // A `u64` field.
    four: u64;

    // A `u128` field.
    five: u128;
}

// A struct with signed integers.
struct SignedInts {

    // An `i8` field.
    one: i8;

    // An `i16` field.
    two: i16;

    // An `i32` field.
    three: i32;

    // An `i64` field.
    four: i64;

    // An `i128` field.
    five: i128;
}


// A struct with primitive types.
struct PrimitiveTypes {

    // A `bool` field.
    one: bool;
}

// A struct with special type fields.
struct SpecialTypes {

    // A `uuid` field.
    one: uuid;

    // A `string` field.
    two: string;

    // A `date` field.
    three: date;
}

// A struct with named type fields.
struct NamedTypes {

    // A `struct` field.
    one: UnsignedInts;
}
