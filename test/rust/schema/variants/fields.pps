// A variant with primitive types.
variant Primitives {

    // A 'u8' field.
    One: u8 = 1;

    // A 'u16' field.
    Two: u16 = 2;

    // A 'u32' field.
    Three: u32 = 3;

    // A 'u64' field.
    Four: u64 = 4;

    // A 'u128' field.
    Five: u128 = 5;
}

// A variant with special types.
variant Specials {

    // A 'uuid' field.
    One: uuid = 1;

    // A 'string' field.
    Two: string = 2;
}

// A variant with named types.
variant Named {

    // A 'Primitives' field.
    One: Primitives = 1;
}
