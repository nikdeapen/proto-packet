// A message with primitive types.
message Primitives {

    // A 'u8' field.
    one: u8 = 1;

    // A 'u16' field.
    two: u16 = 2;

    // A 'u32' field.
    three: u32 = 3;

    // A 'u64' field.
    four: u64 = 4;

    // A 'u128' field.
    five: u128 = 5;
}

// A message with special types.
message Specials {

    // A 'uuid' field.
    one: uuid = 1;

    // A 'string' field.
    two: string = 2;
}

// A message with named types.
message Named {

    // A 'Primitives' field.
    one: Primitives = 1;
}
