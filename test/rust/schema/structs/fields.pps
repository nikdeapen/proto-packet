    // A `u32` field.
    three: u32;

    // A `u64` field.
    four: u64;

    // A `u128` field.
    five: u128;

    // An `i8` field.
    six: i8;

    // An `i16` field.
    seven: i16;

    // An `i32` field.
    eight: i32;

    // An `i64` field.
    nine: i64;

    // An `i128` field.
